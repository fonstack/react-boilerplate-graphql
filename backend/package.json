    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "8.3.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.30.2",
    "prisma-binding": "^2.3.10",
